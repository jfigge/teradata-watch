<drawable
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    class="android.support.wearable.complications.rendering.ComplicationDrawable"
    app:backgroundColor="@color/black"
    app:borderColor="@color/secondary_text"
    app:borderDashGap="2dp"
    app:borderDashWidth="2dp"
    app:borderRadius="12dp"
    app:borderStyle="none"
    app:borderWidth="1dp"
    app:highlightColor="@color/primary_text"
    app:iconColor="@color/secondary_text"
    app:rangedValuePrimaryColor="@color/primary_text"
    app:rangedValueRingWidth="1dp"
    app:rangedValueSecondaryColor="@color/secondary_text"
    app:textColor="@color/white"
    app:textSize="7sp"
    app:textTypeface="afterheadline"
    app:titleColor="@color/primary_text"
    app:titleSize="8sp"
    app:titleTypeface="@font/afterheadline">

    <ambient
        app:highlightColor="@color/white"
        app:iconColor="@color/white"
        app:rangedValuePrimaryColor="@color/white"
        app:titleColor="@color/white"
        app:borderColor="@android:color/transparent"
        app:rangedValueSecondaryColor="@android:color/transparent"/>
</drawable>